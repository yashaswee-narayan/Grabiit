@import url("https://fonts.googleapis.com/css2?family=Kalam:wght@300;400&display=swap");

#body-color {
  background-color: #f5f5f5;
}
a {
  text-decoration: none;
  color: #d6a914;
  font-weight: 600;
}
h3 {
  font-weight: 600;
  font-family: "Poppins", sans-serif;
}
h4 {
  font-weight: 500;
}
h6,
h5 {
  color: #080880;
  font-weight: 600;
  font-family: "Poppins", sans-serif;
}

#title_underline:after {
  content: "";
  display: block;
  width: 25%;
  height: 1.5px;
  background: #7f0000;
  margin: 0 auto;
  border-radius: 3px;
}
/* **************** nav bar**********************************/
.navbar {
  padding: 0.5% 5%;
  background-color: white;
}
.button {
  border: none;
  border-radius: 0.25rem;
  color: var(--secondary);
  background-color: var(--highlight);
  padding: 10px;
  text-align: center;
  text-decoration: none;
  font-family: "Poppins", sans-serif;
  font-size: 14px;
  margin: 4px 2px;
}
.button:hover {
  background-color: rgb(250, 204, 52);
}
/* **************** name box***************************************/
.college-name {
  font-family: "Kalam", cursive;
  color: #080880;
  font-weight: 500;
}
.btn-for-new-test {
  color: #080880;
  font-weight: 600;
  background-color: white;
  padding: 10px;
  text-align: center;
  text-decoration: none;
  font-family: "Poppins", sans-serif;
  font-size: 16px;
  margin: 4px 2px;
}
.btn-for-new-test:hover {
  background-color: rgb(249, 251, 255);
}
.btn-for-view {
  background-color: #e0f1ff;
  color: #080880;
  font-weight: 600;
  padding: 10px;
  text-align: center;
  text-decoration: none;
  font-family: "Poppins", sans-serif;
  font-size: 12px;
  margin: 4px 2px;
}
.btn-for-view:hover {
  background-color: rgb(237, 240, 248);
}
.subjects {
  font-size: small;
  font-weight: 600;
}
.info {
  background-color: #a2a2ee;
}
.profile-pic {
  border-radius: 50%;
  clip-path: circle();
  height: 150px;
  width: 150px;
  border: 10px solid #e0f1ff;
}

/* **************** result section *********************************/

.btn-for-view {
  background-color: #e0f1ff;
  color: #080880;
  font-weight: 600;
  padding: 10px;
  text-align: center;
  text-decoration: none;
  font-family: "Poppins", sans-serif;
  font-size: 12px;
  margin: 4px 2px;
}
.btn-for-view:hover {
  background-color: rgb(237, 240, 248);
}
.subjects {
  font-size: small;
  font-weight: 600;
}
.heading {
  color: #299b07;
}
/************************** footer*************** */
#footer {
  color: #080880;
  font-weight: bold;
  font-family: "Poppins", sans-serif;
  opacity: 0.7;
  font-size: smaller;
}
